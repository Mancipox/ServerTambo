<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletUser</servlet-name>
        <servlet-class>com.tambo.servlets.ServletUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletQuestion</servlet-name>
        <servlet-class>com.tambo.servlets.ServletQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMeeting</servlet-name>
        <servlet-class>com.tambo.servlets.ServletMeeting</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletClass</servlet-name>
        <servlet-class>com.tambo.servlets.ServletClass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletContact</servlet-name>
        <servlet-class>com.tambo.servlets.ServletContact</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletUser</servlet-name>
        <url-pattern>/ServletUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletQuestion</servlet-name>
        <url-pattern>/ServletQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMeeting</servlet-name>
        <url-pattern>/ServletMeeting</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletClass</servlet-name>
        <url-pattern>/ServletClass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletContact</servlet-name>
        <url-pattern>/ServletContact</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
